	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse -c phpstan.neon -l 8 app/",
		"tester": "tester tests -s",
		"check-cs": "phpcs --standard=PSR12 --exclude=Generic.Files.LineLength app",
		"fix-cs": "phpcbf --standard=PSR12 --exclude=Generic.Files.LineLength app"
	}
