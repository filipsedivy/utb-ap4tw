		"nette/robot-loader": "^3.0",
		"nette/security": "^3.0",
		"nette/utils": "^3.0",
		"latte/latte": "^2.5",
		"tracy/tracy": "^2.8",
		"contributte/console": "^0.9",
		"contributte/console-extra": "^0.6",
		"contributte/event-dispatcher": "^0.8",
		"contributte/webpack": "^2.0",
		"nettrine/orm": "^0.8",
		"nettrine/annotations": "^0.7",
		"nettrine/migrations": "^0.7",
		"nettrine/cache": "^0.3",
